	<div class='container-fluid container_section'>
		<div class='col-xs-12'>
			
			<% if current_user.crawls.count <= 1000 %>
				
					<div class='panel panel-round'>
						<div class='panel-heading panel_blue_heading panel_glyph_container'>
							<div class='header_glyph'>
								<span class='glyphicon glyphicon-ok' aria-hidden="true"></span>
							</div>
							<div class='header_text'>
								<p>Marketplace Tutorials</p>
							</div>
						</div>
						<div class='panel-body'>
							<%= render 'marketplace_getting_started_videos' %>
						</div>
					</div>
					<div class='panel panel-round'>
						<div class='panel-heading panel_blue_heading panel_glyph_container'>
							<div class='header_glyph'>
								<span class='glyphicon glyphicon-ok' aria-hidden="true"></span>
							</div>
							<div class='header_text'>
								<p>Tutorials</p>
							</div>
						</div>
						<div class='panel-body'>
							<%= render 'getting_started_videos' %>
							<div class='btn-block free_uni_btn'>
								<%= link_to 'Begin Your Free Month At Source University Here', 'http://source-wave.com/university/enrollment/?rid=p8S0Cx', class:'btn btn-lg btn-block btn-success free_uni_btn_link' %>
